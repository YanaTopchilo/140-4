<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600" prefWidth="1000" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.PersonStageController">
    <children>
        <TableView AnchorPane.topAnchor="15" AnchorPane.bottomAnchor="15" AnchorPane.leftAnchor="15" AnchorPane.rightAnchor="15" fx:id="table" >
            <columns>
                <TableColumn text="Идентификатор" fx:id="idColumn" />
                <TableColumn text="Должность" fx:id="jobtitleColumn" />
                <TableColumn text="ФИО" fx:id="fullnameColumn" />
                <TableColumn text="Телефон" fx:id="phoneColumn" />
                <TableColumn text="Электр. почта" fx:id="emailColumn" />
                <TableColumn text="Кол-во доменов" fx:id="qtyOfDomainsColumn" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
